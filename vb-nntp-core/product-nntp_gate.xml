	<codes>
		<code version="1.0">
			<installcode><![CDATA[$db->hide_errors();

$db->query("
  CREATE TABLE IF NOT EXISTS `" . TABLE_PREFIX . "nntp_auth_cache` (
    `username` varchar(100) NOT NULL,
    `authkey` char(32) NOT NULL,
    `lastactivity` timestamp NOT NULL default CURRENT_TIMESTAMP on update CURRENT_TIMESTAMP,
    `groupslist` text NOT NULL,
    `css` text NOT NULL,
    `menu` text NOT NULL,
    `demotext` text NOT NULL,
    `userid` int(10) unsigned NOT NULL,
    `access` enum('full','demo','none') NOT NULL,
    PRIMARY KEY  (`username`,`authkey`)
  ) ENGINE=MyISAM DEFAULT CHARSET=utf8;
");

$db->query("
  CREATE TABLE IF NOT EXISTS `" . TABLE_PREFIX . "nntp_cache_messages` (
    `groupid` int(10) unsigned NOT NULL,
    `messageid` int(10) unsigned NOT NULL,
    `body` text NOT NULL,
    PRIMARY KEY  (`groupid`,`messageid`)
  ) ENGINE=MyISAM DEFAULT CHARSET=utf8 COMMENT='NNTP messages cache';
");

$db->query("
  CREATE TABLE IF NOT EXISTS `" . TABLE_PREFIX . "nntp_groups` (
    `id` int(10) unsigned NOT NULL auto_increment,
    `plugin_id` char(20) NOT NULL,
    `group_name` char(50) NOT NULL,
    `is_active` enum('yes','no') NOT NULL default 'no',
    `settings` text NOT NULL,
    `map_id` int(10) unsigned NOT NULL default '0' COMMENT 'Map to an existing group id',
    `date_create` timestamp NOT NULL default CURRENT_TIMESTAMP on update CURRENT_TIMESTAMP,
    PRIMARY KEY  (`id`),
    KEY `map_id` (`map_id`),
    KEY `group_name` (`group_name`),
    KEY `date_create` (`date_create`)
  ) ENGINE=MyISAM  DEFAULT CHARSET=utf8 AUTO_INCREMENT=90 ;
");

$db->query("
  CREATE TABLE IF NOT EXISTS `" . TABLE_PREFIX . "nntp_index` (
    `groupid` int(10) unsigned NOT NULL,
    `messageid` int(10) unsigned NOT NULL auto_increment,
    `refid` int(10) unsigned NOT NULL,
    `title` char(255) NOT NULL,
    `datetime` datetime NOT NULL,
    `userid` int(10) unsigned NOT NULL,
    `deleted` enum('no','yes') NOT NULL,
    `messagetype` enum('forum','blog') NOT NULL,
    `postid` int(10) unsigned NOT NULL,
    PRIMARY KEY  (`groupid`,`messageid`),
    UNIQUE KEY `GroupListHelper` (`groupid`,`deleted`,`messageid`),
    KEY `Post` (`messagetype`,`postid`)
  ) ENGINE=MyISAM DEFAULT CHARSET=utf8 AUTO_INCREMENT=1 ;
");

$db->query("
  CREATE TABLE IF NOT EXISTS `" . TABLE_PREFIX . "nntp_stats` (
    `userid` int(10) unsigned NOT NULL,
    `date` date NOT NULL,
    PRIMARY KEY  (`userid`,`date`),
    KEY `date` (`date`)
  ) ENGINE=MyISAM DEFAULT CHARSET=utf8;
");

$db->query("
  CREATE TABLE IF NOT EXISTS `" . TABLE_PREFIX . "nntp_user_settings` (
    `userid` int(10) unsigned NOT NULL,
    `nntp_password` char(32) NOT NULL,
    `use_nntp_password` enum('yes','no') NOT NULL default 'yes',
    PRIMARY KEY  (`userid`)
  ) ENGINE=MyISAM DEFAULT CHARSET=utf8;
");

$db->show_errors();]]></installcode>
